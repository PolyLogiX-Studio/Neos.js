		"prettier": "^2.1.2",
		"typescript": "^4.2.4",
		"typescript-eslint": "^0.0.1-alpha.0"
	},
	"engines": {
		"node": ">=14.0.0"
	},
	"dependencies": {
		"@bombitmanbomb/cloudx": "1.0.0-beta.4",
