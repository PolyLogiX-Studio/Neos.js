	"peerDependencies": {
		"@octokit/rest": "^18.2.0"
	},
	"peerDependenciesMeta": {
		"@octokit/rest": {
			"optional": true
		}
	},
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/plugin-transform-flow-strip-types": "^7.12.1",
